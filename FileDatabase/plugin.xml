<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-jack-file-database" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">

    <name>FileDatabase</name>
    <js-module name="FileDatabase" src="www/FileDatabase.js">
        <clobbers target="window.FileDatabase.file" />
    </js-module>
    
    <platform name="windows">
	    <js-module src="src/windows/FileDatabaseNative.js" name="FileDatabaseNative">
	       	<merges target="" />
	    </js-module>
	    <js-module src="src/windows/FolderDatabase.js" name="FolderDatabase">
	       	<merges target="" />
	    </js-module>	    
	</platform>

</plugin>
